{
  "name": "sentia-manufacturing-dashboard",
  "private": true,
  "version": "1.0.10",
  "description": "Manufacturing Dashboard with React/Vite frontend and Node.js backend",
  "type": "module",
  "engines": {
    "node": ">=24.0.0 <25.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --host --port 3000",
    "dev:server": "nodemon server-fixed.js",
    "dev:mcp": "node mcp-server/enterprise-server-simple.js",
    "build": "vite build",
    "build:staged": "node build-staged.js",
    "build:stage1": "DEPLOYMENT_STAGE=1 node build-staged.js",
    "build:stage2": "DEPLOYMENT_STAGE=2 node build-staged.js",
    "build:stage3": "DEPLOYMENT_STAGE=3 node build-staged.js",
    "build:full": "DEPLOYMENT_STAGE=4 node build-staged.js",
    "render:build": "node build-staged.js",
    "start": "node server-integrated-mcp.js",
    "start:enterprise": "node server-integrated-mcp.js",
    "start:render": "node server-integrated-mcp.js",
    "start:development": "cross-env BRANCH=development node --expose-gc server.js",
    "start:testing": "cross-env BRANCH=test node --expose-gc server.js",
    "start:production": "cross-env BRANCH=production node --expose-gc server.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:security": "vitest run tests/security",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:setup": "playwright install",
    "test:all": "npm run test:run && npm run test:e2e",
    "lint": "npx eslint src server",
    "lint:fix": "eslint . --fix",
    "monitor": "node monitoring-agent.cjs",
    "monitor:status": "node monitoring-agent.cjs --status",
    "monitor:bg": "node monitoring-agent.cjs &",
    "monitor:memory": "node scripts/memory-monitor.js",
    "monitor:memory:live": "node scripts/memory-monitor.js --live",
    "format": "prettier --write \"src/**/*.{js,jsx,json}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json}\" \"*.{js,json,md}\"",
    "deploy:single": "node scripts/enterprise-deployment-automation.js --single",
    "deploy:auto": "node scripts/enterprise-deployment-automation.js --auto",
    "deploy:pipeline": "node scripts/enterprise-deployment-automation.js",
    "typecheck": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run typecheck",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "lint:check": "npx eslint src server --max-warnings 0",
    "prepare": "husky"
  },
