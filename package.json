{
    "name": "sentia-manufacturing-dashboard",
    "version": "2.0.0",
    "description": "Enterprise Manufacturing Dashboard with AI/ML capabilities - September 2025 Edition",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
        "dev:client": "vite --host --port 3000",
        "dev:server": "nodemon server-fixed.js",
        "dev:mcp": "node mcp-server/enterprise-server-simple.js",
        "build": "vite build",
        "preview": "vite preview",
        "start": "NODE_OPTIONS='--max-old-space-size=128' node server-fixed.js",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "db:seed": "node prisma/seed.js",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:e2e": "playwright test",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint \"src\" \"server\"",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\""
    },
