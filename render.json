{
  "services": [
    {
      "type": "web",
      "name": "sentia-manufacturing-dashboard",
      "env": "node",
      "buildCommand": "bash .render-build.sh",
      "startCommand": "node server.js",
      "healthCheckPath": "/health",
      "envVars": [
        {
          "key": "NODE_ENV",
          "value": "production"
        },
        {
          "key": "NODE_VERSION",
          "value": "22.12.0"
        }
      ]
    }
  ]
}