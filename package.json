{
  "name": "sentia-manufacturing-dashboard",
  "version": "1.0.5",
  "description": "Manufacturing Dashboard with React/Vite frontend and Node.js backend",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "nodemon server.js",
    "build": "npx vite build",
    "build:wrapper": "node scripts/render-build-wrapper.js",
    "build:prisma": "npx prisma generate",
    "db:push:safe": "npx prisma db push --accept-data-loss --skip-generate || echo 'Database push handled'",
    "build:db": "npx prisma db push --accept-data-loss --skip-generate || echo 'Database push completed'",
    "prisma:safe-push": "npx prisma db push --accept-data-loss --skip-generate || true",
    "build:render": "bash scripts/render-build.sh",
    "build:render-fix": "bash scripts/render-deploy-fix.sh",
    "render:build": "npm install --legacy-peer-deps && npx vite build && npx prisma generate || echo 'Build completed with warnings'",
    "render:start": "node render-startup.js",
    "start": "node server.js",
    "start:prod": "npx prisma migrate deploy && node server.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:security": "vitest run tests/security",
    "test:api": "vitest run tests/api.test.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:setup": "playwright install",
    "test:all": "npm run test:run && npm run test:e2e",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "monitor": "node monitoring-agent.cjs",
    "monitor:status": "node monitoring-agent.cjs --status",
    "monitor:bg": "node monitoring-agent.cjs &",
    "render:check": "node scripts/check-render-deployment.js",
    "render:monitor": "node scripts/render-deployment-agent.js --monitor",
    "render:fix": "node scripts/render-deployment-agent.js --once",
    "render:env": "node scripts/verify-render-env.js",
    "render:status": "node scripts/monitor-status.js",
    "monitor:stop": "scripts\\stop-monitor-service.bat",
    "monitor:start": "scripts\\start-monitor-service.bat",
    "monitor:logs": "tail -f scripts/deployment-monitor.log",
    "fix:modules": "node scripts/fix-es-modules.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy:single": "node scripts/enterprise-deployment-automation.js --single",
    "deploy:auto": "node scripts/enterprise-deployment-automation.js --auto",
    "deploy:pipeline": "node scripts/enterprise-deployment-automation.js",
    "typecheck": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run typecheck",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "render:setup": "node scripts/render-setup.js",
    "render:deploy": "node scripts/render-deploy.js",
    "render:verify": "node scripts/verify-render-deployment.js",
    "render:validate": "node scripts/validate-env-vars.js",
    "render:migrate": "node scripts/migrate-neon-to-render.sh",
    "render:migrate:win": "scripts\\migrate-neon-to-render.bat",
    "render:health": "node scripts/verify-render-deployment.js",
    "performance": "node scripts/performance-optimizer.js",
    "test:suite": "node tests/automated-test-suite.js",
    "monitor:production": "node monitoring/production-monitor.js",
    "deploy:production": "node scripts/render-deploy.js production",
    "deploy:testing": "node scripts/render-deploy.js testing",
    "deploy:development": "node scripts/render-deploy.js development",
    "deploy:all": "npm run render:setup && npm run render:deploy && npm run render:verify",
    "deploy:dev": "node scripts/render-deploy.js development",
    "deploy:test": "node scripts/render-deploy.js testing",
    "deploy:prod": "node scripts/render-deploy.js production",
    "health:check": "node scripts/health-monitor.js",
    "health:monitor": "node scripts/health-monitor.js --watch",
    "qa:full": "npm run health:check && npm run lint && npm run test:run",
    "autonomous:watchdog": "node services/observability/autonomous-watchdog.js",
    "autonomous:start": "node tests/autonomous/autonomous-scheduler.js",
    "test-agent:deploy": "node deploy-test-agent.js",
    "test-agent:start": "cmd /c \"set AUTONOMOUS_SCHEDULER_MODE=render && node tests/autonomous/autonomous-scheduler.js\"",
    "autonomous:stop": "taskkill /F /FI \"WINDOWTITLE eq Autonomous*\" /T",
    "autonomous:24-7": "node tests/autonomous/start-24-7-service.js",
    "autonomous:service": "node tests/autonomous/service-manager.js",
    "autonomous:status": "type tests\\autonomous\\logs\\scheduler-status.json 2>nul || echo No status available",
    "autonomous:health": "type tests\\autonomous\\logs\\service-manager.log 2>nul | tail -20 || echo No health logs available",
    "autonomous:logs": "type tests\\autonomous\\logs\\scheduler.log 2>nul | tail -50 || echo No scheduler logs available",
    "autonomous:deploy-logs": "type tests\\autonomous\\logs\\deployment.log 2>nul | tail -20 || echo No deployment logs available",
    "test:autonomous": "playwright test --config=playwright.autonomous.config.js",
    "dev:clean": "rimraf node_modules/.vite dist",
    "dev:fresh": "npm run dev:clean && npm install && npm run dev",
    "dev:quick": "concurrently \"npm run serve\" \"npm run dev:client\"",
    "serve:static": "npx serve dist -p 3000",
    "dev:production-test": "npm run build && npm run serve:static",
    "mcp:start": "cd mcp-server && npm start",
    "mcp:dev": "cd mcp-server && npm run dev",
    "mcp:test": "cd mcp-server && npm run test",
    "mcp:health": "cd mcp-server && npm run health",
    "ai:full-stack": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run mcp:dev\" --names \"server,client,mcp\" --prefix-colors \"blue,green,yellow\"",
    "ai:production": "concurrently \"npm run start\" \"npm run mcp:start\" --names \"main,mcp\" --prefix-colors \"blue,yellow\""
  },
