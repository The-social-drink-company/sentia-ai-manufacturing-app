  "private": true,
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "vite --host --port 3000",
    "dev:server": "nodemon server/index.js",
    "dev:server:ts": "tsx watch server/index.ts",
    "build": "prisma generate && vite build",
    "build:dev": "prisma generate && vite build --mode development",
    "build:test": "prisma generate && vite build --mode testing",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "node server/index.js",
    "start:render": "prisma generate && node server/index.js",
    "start:full": "node server.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint src server --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint src server --ext .js,.jsx,.ts,.tsx --max-warnings 20",
    "lint:fix": "eslint src server --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\" \"server/**/*.{js,ts,json}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\" \"server/**/*.{js,ts,json}\" \"*.{js,json,md}\"",
    "typecheck": "tsc --project tsconfig.server.json --noEmit",
    "postinstall": "prisma generate",
    "lint:gating": "set ESLINT_USE_FLAT_CONFIG=false&&npx eslint --no-eslintrc -c .eslintrc.gating.json src/components/features/FeatureGate.tsx src/components/features/FeatureTooltip.tsx src/components/features/TierBadge.tsx src/components/features/UsageLimitIndicator.tsx src/components/features/UpgradeModal.tsx src/components/features/index.ts src/hooks/useFeatureAccess.ts",
    "typecheck:gating": "tsc --project tsconfig.gating.json --noEmit"
  },
