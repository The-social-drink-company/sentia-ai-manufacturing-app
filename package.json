{
  "name": "sentia-manufacturing-dashboard",
  "version": "2.0.0",
  "description": "Enterprise Manufacturing Dashboard with AI/ML capabilities - September 2025 Edition",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --host --port 3000",
    "dev:server": "nodemon server-fixed.js",
    "dev:mcp": "node mcp-server/enterprise-server-simple.js",
    "dev:all": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:mcp\"",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "build:render": "npm install --prefer-offline --no-audit --progress=false && NODE_OPTIONS='--max-old-space-size=4096' npm run build && npx prisma generate",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "start": "NODE_OPTIONS='--max-old-space-size=128' node server-fixed.js",
    "start:production": "NODE_ENV=production npm run start",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .vite",
    "postinstall": "prisma generate"
  },
