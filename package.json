{
    "name": "sentia-manufacturing-dashboard",
    "version": "2.0.0",
    "description": "Sentia Manufacturing Dashboard monorepo for client and server",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
        "dev:client": "vite --host --port 3000",
        "dev:server": "cross-env NODE_ENV=development nodemon server-fixed.js",
        "build": "npm run build:client && npm run build:server",
        "build:client": "cross-env NODE_ENV=production vite build",
        "build:server": "echo 'Server build not required for production'",
        "start": "cross-env NODE_ENV=production node server-fixed.js",
        "test": "vitest",
        "test:e2e": "playwright test",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate deploy",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"server/**/*.js\"",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@anthropic-ai/sdk": "latest",
        "@clerk/clerk-sdk-node": "5.1.6",
        "@google/generative-ai": "latest",
