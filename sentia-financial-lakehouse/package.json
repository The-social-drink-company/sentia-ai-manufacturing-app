{
  "name": "sentia-financial-lakehouse",
  "version": "1.0.0",
  "description": "Enterprise Financial Intelligence Platform - Data Lakehouse & AI Orchestration",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:lakehouse\" \"pnpm dev:metrics\" \"pnpm dev:ui\"",
    "dev:lakehouse": "node --watch services/lakehouse/index.js",
    "dev:metrics": "node --watch services/metrics/index.js",
    "dev:ui": "vite",
    "build": "pnpm build:ui && pnpm build:services",
    "build:ui": "vite build",
    "build:services": "node scripts/build-services.js",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "validate": "pnpm lint && pnpm typecheck && pnpm test:run",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@apache-arrow/ts": "^14.0.0",
    "@clerk/express": "^1.5.0",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.7.7",
    "bullmq": "^5.29.0",
    "cors": "^2.8.5",
    "duckdb": "^1.1.0",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "parquet-wasm": "^0.6.0",
    "pino": "^9.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zod": "^3.23.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.16.0",
    "prettier": "^3.4.0",
    "prisma": "^5.22.0",
    "typescript": "^5.7.0",
    "vite": "^4.5.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}