{
    "name": "sentia-manufacturing-dashboard",
    "version": "2.0.0",
    "description": "Enterprise Manufacturing Dashboard with AI/ML capabilities - September 2025 Edition",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=24.0.0 <25.0.0"
    },
    "scripts": {
        "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
        "dev:client": "vite --host --port 3000",
        "dev:server": "nodemon server-fixed.js",
        "dev:mcp": "node mcp-server/enterprise-server-simple.js",
        "build": "vite build",
        "preview": "vite preview",
        "start": "cross-env NODE_OPTIONS=--max-old-space-size=128 node server-fixed.js",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "db:seed": "node prisma/seed.js",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:e2e": "playwright test",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint --config .eslintrc.enterprise.cjs \"src/**/*.{js,jsx}\" --fix",
        "lint:check": "eslint --config .eslintrc.enterprise.cjs \"src/**/*.{js,jsx}\"",
        "format": "prettier --write \"src/**/*.{js,jsx,json}\" \"*.{js,json,md}\"",
        "format:check": "prettier --check \"src/**/*.{js,jsx,json}\" \"*.{js,json,md}\"",
        "typecheck": "tsc --noEmit",
        "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" \"context/README.md\" \"context/development-methodology/repository-reset-2025-09-25.md\" \"context/new_product_prompt_answers.md\" \".github/SPECKIT.md\" \"spec-kit/specs/001-sentia-manufacturing-dashboard/*.md\""
    },
